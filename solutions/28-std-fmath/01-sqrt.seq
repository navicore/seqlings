use std:fmath

: test-sqrt-perfect ( -- )
    25.0 fmath.sqrt
    5.0 f.= test.assert
;

: test-sqrt-100 ( -- )
    100.0 fmath.sqrt
    10.0 f.= test.assert
;

: hypotenuse ( Float Float -- Float )
    dup f.* swap dup f.* f.+ fmath.sqrt
;

: test-hypotenuse ( -- )
    3.0 4.0 hypotenuse
    5.0 f.= test.assert
;
