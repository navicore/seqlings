: test-list-length ( -- )
    "one two three four" " " string.split list.length
    4 test.assert-eq
;

: test-list-first ( -- )
    "alpha beta gamma" " " string.split 0 variant.field-at
    "alpha" string.equal? test.assert
;

: test-list-last ( -- )
    "alpha beta gamma" " " string.split 2 variant.field-at
    "gamma" string.equal? test.assert
;

: test-list-empty ( -- )
    "single" " " string.split list.empty?
    not test.assert
;
