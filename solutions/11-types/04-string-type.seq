: test-string-length ( -- )
    "hello" string.length
    5 test.assert-eq
;

: test-string-concat ( -- )
    "hello" " world" string.concat
    "hello world" string.equal? test.assert
;

: test-string-empty ( -- )
    "" string.empty?
    1 test.assert-eq
;

: test-string-not-empty ( -- )
    "hi" string.empty?
    0 test.assert-eq
;
