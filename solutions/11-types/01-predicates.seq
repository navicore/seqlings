: add-integers ( Int Int -- Int )
    i.add
;

: test-int-type ( -- )
    10 20 add-integers
    30 test.assert-eq
;

: add-floats ( Float Float -- Float )
    f.add
;

: test-float-type ( -- )
    1.5 2.5 add-floats
    4.0 f.= test.assert
;

: concat-strings ( String String -- String )
    string.concat
;

: test-string-type ( -- )
    "Hello, " "World!" concat-strings
    "Hello, World!" string.equal? test.assert
;
